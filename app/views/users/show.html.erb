 <div class="contents row" >
    <p><%= @nickname %>さんの日記</p>
  
  <table>
    <thead>
      <tr>
        <th>タイトル</th>
        <th>内容</th>
        <th>登録日時</th>
        <th>更新日時</th>
     
      </tr>
    </thead>

    <body>
     <% @diaries.each do |diary| %>
      <tr>
        <td><%= diary.title %></td>
        <td><%= diary.body %></td>
        <td><%= diary.created_at %>
        <td><%= diary.updated_at %>
        <%# <td><%= link_to 'Show', diary %></td> 
        <td><%= link_to '編集', edit_diary_path(diary) %></td>
        <td><%= link_to '削除', diary, method: :delete, data: { confirm: '削除しても構いませんか?' } %></td>
        
      </tr>
     <% end %>
      <%= link_to '日記を新しく書く', new_diary_path %> 
    <tbody>
  </table>